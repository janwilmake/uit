{
  "$schema": "./plugins.schema.json",
  "plugins": {
    "issues": {
      "title": "Issues",
      "domain": "github.com",
      "type": "api",
      "url": "https://cache.forgithub.com/{owner}/{repo}/issues",
      "description": "Loads issues of a repo"
    },

    "pulls": {
      "title": "Pull Requests",
      "domain": "github.com",
      "type": "api",
      "url": "https://cache.forgithub.com/{owner}/{repo}/pulls",
      "description": "Load PRs of a repo"
    },

    "discussions": {
      "title": "Discussions",
      "domain": "github.com",
      "type": "api",
      "url": "https://cache.forgithub.com/{owner}/{repo}/discussions",
      "description": "Load Discussions of a repo"
    },

    "branches": {
      "type": "api",
      "description": "Gets last commits and contributor info",
      "url": "https://log.forgithub.com/{owner}/{repo}/branches",
      "title": "Commits",
      "domain": "github.com"
    },

    "commits": {
      "type": "api",
      "description": "Gets last commits and contributor info",
      "url": "https://log.forgithub.com/{owner}/{repo}/commits",
      "title": "Commits",
      "domain": "github.com"
    },

    "releases": {
      "title": "Releases",
      "url": "https://log.forgithub.com/{owner}/{repo}/releases",
      "type": "api",
      "domain": "github.com"
    },

    "actions": {
      "title": "Actions",
      "domain": "github.com",
      "type": "api",
      "url": "https://actions.forgithub.com/{owner}/{repo}"
    },

    "swc": {
      "title": "SWC",
      "type": "formdata-transformer",
      "url": "https://swc.uithub.com",
      "description": "Uses SWC to parse TypeScript/JavaScript into types, functions, imports/exports, and variables."
    },

    "typedoc": {
      "title": "Typedoc",
      "description": "Uses typedoc to generate a markdown overview.",
      "type": "api",
      "domain": "github.com",
      "url": "https://typedoc.uithub.com"
    },

    "diagram": {
      "title": "Diagram",
      "type": "api",
      "url": "https://diagram.forgithub.com/{owner}/{repo}",
      "description": "Uses LLMs to generate a diagram for the codebase."
    },

    "questions": {
      "title": "Questions",
      "type": "api",
      "url": "https://questions.forgithub.com/{owner}/{repo}",
      "description": "Uses LLMs to come up with useful questions about the repo to chat about."
    },

    "threads": {
      "title": "X Threads",
      "type": "api",
      "domain": "github.com",
      "url": "https://threads.forgithub.com/{owner}/{repo}",
      "description": "Finds relevant X threads using XYMake"
    },

    "domain": {
      "title": "Domain",
      "url": "https://domain.uithub.com/{owner}/{repo}",
      "type": "api",
      "description": "Finds potential hosted domains using config files"
    },

    "openapis": {
      "title": "OpenAPIs",
      "type": "api",
      "url": "https://openapis.uithub.com/{owner}/{repo}",
      "description": "Finds OpenAPIs in a file system"
    }
  }
}
